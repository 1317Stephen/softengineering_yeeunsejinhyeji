import java.awt.Dimension;
import java.awt.FlowLayout;
import java.text.DateFormat;
import java.text.ParseException;
import java.text.SimpleDateFormat;
//import java.util.Date;
//import java.util.Scanner;
import java.util.Vector;

import javax.swing.JFormattedTextField;
import javax.swing.JFrame;
import javax.swing.JLabel;
import javax.swing.JPanel;

public class VectorStr_account_date {

	public static void main(String[] args) throws ParseException
	{
		// TODO Auto-generated method stub
		int size = 2;
		String[] str = new String[2];
		Vector <String> account_date2 = new Vector<String>(size);
/*		Vector <String> account_item = new Vector<String>();
		Vector <Integer> account_price = new Vector<Integer>();
	*/	
		
		JFrame jf = new JFrame("date");
		JPanel jp = new JPanel();
		JLabel[] lb = new JLabel[2];
		DateFormat format2 = new SimpleDateFormat("2013-10-11");
		JFormattedTextField d_txtfield = new JFormattedTextField(format2);
		
		jf.setLayout(new FlowLayout());
		jf.setBounds(120,120,250,150);
		
		
		d_txtfield.setPreferredSize( new Dimension( 200, 24 ) );
		for(int i=0; i<2; i++)
		{
			str[i] = "df";
			account_date2.add(str[i]);
			lb[i] = new JLabel(str[i]);
			jp.add(lb[i]);
		}
		
		
		jf.add(jp);
		jf.add(d_txtfield);
		jf.setVisible(true);
		
		
		
	}

}
